{
  "compilerOptions": {
    "target": "ES2015",
    "moduleResolution": "node",
    "module": "CommonJS",
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "noImplicitAny": false
  
  }
}
